[
  {
    "id": "test-hook",
    "execute-command": "/root/test-command.sh",
    "command-working-directory": "/root",
    "pass-arguments-to-command": [
      {
        "source": "payload",
        "name": "repository.repo_name"
      },
      {
        "source": "payload",
        "name": "push_data.tag"
      },
      {
        "source": "url",
        "name": "tcBuildTypeId"
      }
    ],
    "trigger-rule": {
      "and": [
        {
          "match": {
            "type": "value",
            "value": "{{ getenv "WEBHOOK_AUTH" | js }}",
            "parameter": {
              "source": "url",
              "name": "auth"
            }
          }
        }
      ]
    }
  }
]
